{
  "taskRoleArn": "arn:aws:iam::905418223611:role/beworking-ecs-task-role",
  "family": "beworking-stripe-service-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::905418223611:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "beworking-stripe-service",
      "image": "905418223611.dkr.ecr.eu-north-1.amazonaws.com/beworking-stripe-service:main",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8081,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "APP_TENANT",
          "value": "default"
        }
      ],
      "secrets": [
        {
          "name": "STRIPE_SECRET_KEY",
          "valueFrom": "arn:aws:secretsmanager:eu-north-1:905418223611:secret:beworking-stripe-secret-key-jSK7v5"
        },
        {
          "name": "STRIPE_WEBHOOK_SECRET",
          "valueFrom": "arn:aws:secretsmanager:eu-north-1:905418223611:secret:beworking-stripe-webhook-secret-KAyVNv"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/beworking-stripe-service-task",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
